        <item name="mainOnlyMinTextSize">@dimen/keyboard_main_only_min_text_size</item>
        <item name="mainTextSize">@dimen/keyboard_main_text_size</item>
        <item name="altTextSize">@dimen/keyboard_alt_text_size</item>
        <item name="mainTextColor">@color/key_main_text_color</item>
        <item name="altTextColor">@color/key_alt_text_color</item>
